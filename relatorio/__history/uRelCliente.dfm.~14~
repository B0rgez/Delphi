  object QryClientes: TFDQuery
    Active = True
    Connection = dtmConexao.ConexaoDBase
    SQL.Strings = (
      'SELECT clientes.clienteID, '
      '       clientes.nome, '
      '       clientes.email, '
      '       clientes.telefone, '
      '       clientes.cep  '
      ' FROM clientes'
      'ORDER BY clientes.nome'
      '')
    Left = 632
    Top = 232
